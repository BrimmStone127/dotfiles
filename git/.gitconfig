[user]
	email = claybrimm@gmail.com
	name = Brimmstone127
[init]
	defaultBranch = main
[core]
	pager = less -R
	editor = nvim
	hooksPath = /home/clay/.config/git/hooks
[alias]
	s = status -s
	lg = log --oneline --graph --decorate --all
	last = log -1 HEAD
	amend = commit --amend --no-edit
	undo = reset --soft HEAD~1
	br = branch -v
